    "import re\n",
    "\n",
    "text = \"contact me at 123-456-7890 or 987-654-3210\"\n",
    "\n",
    "# Regular expression to match phone numbers in the format xxx-xxx-xxxx\n",
    "pattern = r\"\\d{3}-\\d{3}-\\d{4}\"\n",
    "\n",
    "digits = re.findall(pattern, text)\n",
    "# Extract the first phone number\n",
    "\n",
    "print(digits)"
   ]
